{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/productos",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(!(@.nombre) || @.nombre == '' || @.precio <= 0 || !(@.usuario.id))]"
      }
    ]
  },
  "response": {
    "status": 400,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "postProductos-400.json"

  }
}