{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/usuarios",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.password =~ /^.{0,3}$/ || @.saldo <= 0 || !(@.email =~ /.*@.*/))]"
      }
    ]
  },
  "response": {
    "status": 400,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "postUsuarios-400.json"

  }
}