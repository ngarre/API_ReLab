{
  "priority": 1,
  "request": {
    "method": "POST",
    "url": "/compraventas",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(!(@.productoId) || @.productoId == '' || !(@.compradorId) || @.compradorId == '' || !(@.vendedorId) || @.vendedorId == '' || @.precioFinal <= 0)]"
      }
    ]
  },
  "response": {
    "status": 400,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "postCompraventas-400.json"
  }
}
